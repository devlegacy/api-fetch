    "cors": "^2.8.5",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "mime-types": "^2.1.32",
    "morgan": "^1.10.0",
    "multer": "^1.4.3",
    "netlify-lambda": "^2.0.14",
    "normalize.css": "^8.0.1",
    "serverless-http": "^2.7.0"
  }
}
