{
  "name": "api-fetch",
  "version": "0.0.1",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "npm-run-all --parallel watch serve:local serve:api",
    "build": "cross-env NODE_ENV=production webpack --config webpack.config.js --mode=production --env presets=typescript",
    "watch": "cross-env NODE_ENV=development npm run build -- --watch --mode=development ",
    "serve:local": "http-server ./public -p 8080 -e -g -c-1",
    "serve:api": "node ./server/index.js",
    "package:update": "ncu -u && npm i -D favicons-webpack-plugin@next html-webpack-plugin@next && npm i && npm audit fix --force",
    "clean": "rm -Rf ./public/js/*.js ./public/*.png ./public/*.xml ./public/*.json ./public/*.webapp ./public/*.ico ./public/*.svg"
  },
  "keywords": [],
  "author": "Samuel R. <samuel@devexteam.com> (https://devexteam.com)",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^14.14.22",
    "cross-env": "^7.0.3",
    "favicons": "^6.2.0",
    "favicons-webpack-plugin": "^5.0.0-alpha.11",
    "html-webpack-plugin": "^5.0.0-beta.6",
    "http-server": "^0.12.3",
    "npm-run-all": "^4.1.5",
    "ts-loader": "^8.0.14",
    "typescript": "^4.1.3",
    "webpack": "^5.18.0",
    "webpack-cli": "^4.4.0",
    "webpack-merge": "^5.7.3",
    "workbox-precaching": "^6.0.2",
    "workbox-webpack-plugin": "^6.0.2"
  },
  "dependencies": {
    "cookie-parser": "^1.4.5",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "mime-types": "^2.1.28",
    "morgan": "^1.10.0",
    "multer": "^1.4.2"
  }
}
